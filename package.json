{
  "name": "graphql-binary",
  "version": "0.1.0",
  "description": "GraphQL binary protocol for smaller network traffic and parsing performance",
  "main": "index.js",
  "repository": "https://github.com/esseswann/graphql-binary",
  "author": "jsus <esseswann@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest --watch --runInBand",
    "dev": "nodemon --exec babel-node"
  },
  "dependencies": {
    "capitalize": "2.0.3",
    "graphql": "15.3.0",
    "lodash": "4.17.20"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/node": "7.10.5",
    "@babel/preset-env": "7.11.5",
    "babel-jest": "26.3.0",
    "babel-plugin-import-graphql": "2.7.0",
    "babel-plugin-module-resolver": "4.0.0",
    "graphql-query-compress": "1.2.4",
    "graphql-tools": "5.0.0",
    "jest": "26.4.2",
    "nodemon": "2.0.4"
  },
  "babel": {
    "plugins": [
      "import-graphql",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
