{
  "name": "graphql-binary",
  "version": "0.1.0",
  "description": "GraphQL binary protocol for smaller network traffic and parsing performance",
  "main": "index.js",
  "repository": "https://github.com/esseswann/graphql-binary",
  "author": "jsus <esseswann@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest --watch --runInBand",
    "dev": "nodemon --exec babel-node --extensions .ts"
  },
  "dependencies": {
    "graphql": "15.5.1",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/node": "7.14.7",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "@types/jest": "26.0.24",
    "babel-jest": "27.0.6",
    "babel-plugin-import-graphql": "2.8.1",
    "babel-plugin-module-resolver": "4.1.0",
    "graphql-query-compress": "1.2.4",
    "graphql-tools": "7.0.5",
    "jest": "27.0.6",
    "nodemon": "2.0.12",
    "prettier": "2.3.2"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "graphql"
    ]
  },
  "babel": {
    "plugins": [
      "import-graphql",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ],
    "presets": [
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
