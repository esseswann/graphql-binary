{
  "name": "graphql-binary",
  "version": "0.1.0",
  "description": "GraphQL binary protocol for smaller network traffic and parsing performance",
  "main": "index.js",
  "repository": "https://github.com/esseswann/graphql-binary",
  "author": "jsus <esseswann@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest --watch --runInBand",
    "dev": "nodemon --exec ts-node"
  },
  "dependencies": {
    "graphql": "15.5.1",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/node": "16.4.2",
    "graphql-query-compress": "1.2.4",
    "graphql-tools": "7.0.5",
    "jest": "27.0.6",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,graphql"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "graphql"
    ]
  }
}
